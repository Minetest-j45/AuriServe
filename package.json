{
	"name": "auriserve",
	"version": "1.0.0",
	"description": "",
	"main": "server/src/Main.js",
	"bin": "server/src/Main.js",
	"scripts": {
		"dev": "npm run dev-admin & npm run dev-server",
		"dev-admin": "cd admin; webpack --watch --progress --config webpack.ts",
		"dev-server": "nodemon --config server/nodemon.json",
		"dev-clean": "rm -r common; rm -r admin; rm -r server; rm -rf node_modules",
		"build": "npm run build-admin && npm run build-server",
		"build-admin": "cd admin; webpack --mode=production --progress --config webpack.ts",
		"build-server": "npm run lint-server && tsc --project server/tsconfig.json && pkg package.json --options expose-gc --out-path build && npm run build-clean",
		"build-clean": "find server/src -name '*.js' -type f -delete && find common -name '*.js' -type f -delete",
		"lint-server": "cd server; eslint -c .eslintrc.js 'src/**/*.ts'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Aurailus/AuriServe.git"
	},
	"keywords": [
		"CMS",
		"Modern",
		"AuriServe"
	],
	"author": "Auri Collings",
	"license": "AGPL-3.0-only",
	"bugs": {
		"url": "https://github.com/Aurailus/AuriServe/issues"
	},
	"pkg": {
		"assets": [
			"server/views/*",
			"admin/res/**/*",
			"admin/build/*"
		]
	},
	"homepage": "https://github.com/Aurailus/AuriServe#readme",
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
		"@babel/plugin-transform-react-jsx": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-typescript": "^7.10.4",
		"@types/bcryptjs": "^2.4.2",
		"@types/compression": "^1.7.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/express-fileupload": "^1.1.3",
		"@types/js-cookie": "^2.2.6",
		"@types/log4js": "^2.3.5",
		"@types/minimist": "^1.2.0",
		"@types/mongodb": "^3.5.28",
		"@types/node": "^14.11.8",
		"@types/pug": "^2.0.4",
		"@types/react-router-dom": "^5.1.6",
		"@types/rimraf": "^3.0.0",
		"@types/sass": "^1.16.0",
		"@types/webpack": "^4.41.22",
		"@typescript-eslint/eslint-plugin": "^4.4.1",
		"@typescript-eslint/parser": "^4.1.1",
		"babel-loader": "^8.1.0",
		"css-loader": "^5.0.0",
		"eslint": "^7.11.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-jsdoc": "^30.6.5",
		"file-loader": "^6.1.1",
		"fork-ts-checker-webpack-plugin": "^5.2.0",
		"moment-locales-webpack-plugin": "^1.2.0",
		"nodemon": "^2.0.6",
		"pkg": "^4.4.9",
		"sass-loader": "^9.0.3",
		"style-loader": "^1.3.0",
		"ts-loader": "^8.0.5",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.3",
		"webpack": "^4.44.1",
		"webpack-bundle-analyzer": "^3.9.0",
		"webpack-cli": "^3.3.12",
		"webpack-livereload-plugin": "^2.3.0",
		"webpack-merge": "^5.2.0"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.5",
		"core-js": "^3.6.5",
		"decache": "^4.6.0",
		"express": "^4.17.1",
		"express-fileupload": "^1.2.0",
		"js-cookie": "^2.2.1",
		"log4js": "^6.3.0",
		"minimist": "^1.2.5",
		"moment": "^2.29.1",
		"mongodb": "^3.6.0",
		"preact": "^10.4.8",
		"preact-render-to-string": "^5.1.10",
		"preact-router": "^3.2.1",
		"preact-transitioning": "^1.0.2",
		"prompt": "^1.0.0",
		"pug": "^3.0.0",
		"react-router-dom": "^5.2.0",
		"recursive-watch": "^1.1.4",
		"rimraf": "^3.0.2",
		"sass": "^1.27.0"
	}
}
